@model Shopping.Data.Entities.City

@{
    ViewData["Title"] = "Delete";
}
<flash dismissable="true" />

<h1>Borrar</h1>

<h3>¿Esta seguro que quiere borrar la Ciudad? </h3>
<div>

    <partial name="_CityDetails" />

    <form asp-action="Deletecity">
        <input type="hidden" asp-for="Id" />
        <input type="submit" value="Borrar" class="btn btn-outline-danger" />
        <a asp-action="DetailsState"  asp-route-id="@Model?.State.Id" class="btn btn-outline-success">Regresar</a>
    </form>
</div>